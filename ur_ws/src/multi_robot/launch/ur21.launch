<launch>


<arg name="limited" default="true"  doc="If true, limits joint range [-PI, PI] on all joints." />
  <arg name="paused" default="false" doc="Starts gazebo in paused mode" />
  <arg name="gui" default="true" doc="Starts gazebo gui" />

  <!-- send robot urdf to param server -->
  <include file="$(find ur_description1)/launch/ur10_upload.launch">
    <arg name="limited" value="$(arg limited)"/>
  </include>

  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot2 -x 0.170 -y -0.3 -z 0.8 -Y -1.57 -J Ashoulder_pan_joint 0.0 -J Ashoulder_lift_joint 0.0 -J Aelbow_joint 0.0 -J Awrist_1_joint 0.0 -J Awrist_2_joint 0.0 -J Awrist_3_joint 0.0" respawn="false" output="screen" />

  <include file="$(find ur_gazebo1)/launch/controller_utils.launch"/>


  <!-- start this controller -->
<!--<rosparam file="$(find ur_gazebo)/controller/arm_controller_ur10.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn arm_controller" respawn="false" output="screen"/>
-->
  <!-- load other controllers -->
  <!--<node name="ros_control_controller_manager" pkg="controller_manager" type="controller_manager" respawn="false" output="screen" args="load joint_group_position_controller" />
-->
  <arg name="sim" default="true" />
  
  <arg name="debug" default="false" />

<rosparam file="$(find ur_gazebo1)/controller/joint_vel_controllers.yaml" command="load"/>

<!-- TO BE UNCOMMENTED -->

<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="/robot2" args="Ashoulder_pan_joint_velocity_controller Ashoulder_lift_joint_velocity_controller Aelbow_joint_velocity_controller Awrist_1_joint_velocity_controller Awrist_2_joint_velocity_controller Awrist_3_joint_velocity_controller"/>










